        "eslint": "8.57.0",
        "eslint-plugin-react": "7.34.0",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-refresh": "0.4.5",
        "gh-pages": "6.1.1",
        "husky": "9.0.11",
